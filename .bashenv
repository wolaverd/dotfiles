HISTCONTROL=ignoreboth
HISTSIZE=1000
HISTFILESIZE=2000

EDITOR=vim
VISUAL=subl
GIT_EDITOR="subl --wait --new-window"
SYSTEMD_EDITOR=vim

PYTHONDONTWRITEBYTECODE=1
PYTHONPATH=/usr/local/lib/user_py27
JYTHONHOME=/usr/share/jython
JYTHON_HOME=$JYTHONHOME

PGDATABASE=wms801

JAVA_HOME=/usr/lib/jvm/java-8-oracle
JDK_HOME=$JAVA_HOME
JRE_HOME="${JAVA_HOME}/jre"
JAVA_TOOL_OPTIONS=-Dfile.encoding="UTF-8"

####
# Just wanted to see if this would work. Don't judge me.

while IFS= read -u 9 -r line; do
    if [[ $line =~ ^#### ]]; then
	break
    elif [[ -z $line ]]; then
    	continue
    fi
    eval "export ${line%%=*}"
done 9< <(cat< "$(readlink -f "${BASH_SOURCE[0]}")")
