lefix () { sed -i 's/\r//g' "$@"; }
mkcd () { mkdir -pv "$1" && cd "$1"; }
dbg () { printf "DEBUG: \$_ == %q\n" "$_" >&2; }

mantoc () {
	local manpage
	manpage="$1"

	man "$manpage" | cat | grep -o '^[A-Z][A-Z -]\+'
}

mountext () {
	
	if [[ -f $1 ]]; then
		sudo mount "$1" /run/media/neeto -o \
			user,rw,umask=111,dmask=000
	fi
}
